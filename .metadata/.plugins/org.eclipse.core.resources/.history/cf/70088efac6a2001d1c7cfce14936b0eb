package program_asked_interview;

public class Reverse_string {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String name="sneha";
		String rev=null;
		int len=name.length();
		for(int i=len-1;i>=0;i--)
		{
			rev=rev+name.charAt(i);
		}
		
		System.out.println(rev);

	}

}
